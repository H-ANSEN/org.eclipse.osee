<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" activate="start" name="org.eclipse.osee.orcs.db.internal.proxy.ResourceDataProxyFactory">
   <implementation class="org.eclipse.osee.orcs.db.internal.proxy.ResourceDataProxyFactory"/>
   <reference interface="org.eclipse.osee.logger.Log" name="Log" 
   	cardinality="1..1"  
   	policy="static" 
   	bind="setLogger" />
   <service>
      <provide interface="org.eclipse.osee.orcs.core.ds.DataProxyFactory"/>
   </service>
   <property name="proxy.factory.alias" type="String">DefaultAttributeDataProvider
ClobAttributeDataProvider
UriAttributeDataProvider
   </property>
   <reference bind="setResourceLocator" cardinality="1..1" interface="org.eclipse.osee.framework.resource.management.IResourceLocatorManager" name="IResourceLocatorManager" policy="static"/>
   <reference bind="setResourceManager" cardinality="1..1" interface="org.eclipse.osee.framework.resource.management.IResourceManager" name="IResourceManager" policy="static"/>
</scr:component>
