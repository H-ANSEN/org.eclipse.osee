<?xml version="1.0"?>
<component name="authentication.manager">
	<implementation class="org.eclipse.osee.framework.session.management.internal.AuthenticationManager"/>
	<service>
		<provide interface="org.eclipse.osee.framework.session.management.IAuthenticationManager"/>
	</service>
	<reference name="resource.provider"
		interface="org.eclipse.osee.framework.session.management.IAuthenticationProvider"
		bind="addAuthenticationProvider"
		unbind="removeAuthenticationProvider"
		cardinality="0..n"
		policy="dynamic"/>
</component>