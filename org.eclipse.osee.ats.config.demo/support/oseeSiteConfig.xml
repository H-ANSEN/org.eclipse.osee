<OSEEConfig>
<Developer/>
	<Mode>Production</Mode>
	<ServiceLookup>
		<Group>LBA_DEVELOPMENT</Group>	
	</ServiceLookup>
	<Logger>
		<Name>com.sun.jini.thread.TaskManager</Name>
		<Level>OFF</Level>
	</Logger>
	<Logger>
		<Name>net.jini.discovery.LookupLocatorDiscovery</Name>
		<Level>OFF</Level>
	</Logger>
	<Logger>
		<Name>org.eclipse.osee.framework.plugin.core.server.ClassServer</Name>
		<Level>OFF</Level>
	</Logger>
	<Logger>
		<Name>com.sun.jini.reggie.RegistrarProxy</Name>
		<Level>OFF</Level>
	</Logger>

	<DatabaseInfo id="postgresqlInfo">
		<DatabaseHome key="#DBHOME#" />
		<DatabaseName key="#DBNAME#" value="OSEE" />
		<DatabaseType key="#TYPE#" value="postgresql" />
		<Prefix key="#PREFIX#" value="jdbc:postgresql" />
		<UserName key="#USERNAME#" value="osee" />
		<Password key="#PASSWORD#" value="osee" />
		<Host key="#HOST#" value="@AvailableDbServices.hostAddress" />
		<Port key="#PORT#" value="@AvailableDbServices.port" />
	</DatabaseInfo>
	
	<DatabaseInfo id="LbaOracleServerTest2">
		<DatabaseHome key="#DBHOME#" />
		<DatabaseName key="#DBNAME#" value="tlba5" />
		<DatabaseType key="#TYPE#" value="oracle" />
		<Prefix key="#PREFIX#" value="jdbc:oracle:thin" />
		<UserName key="#USERNAME#" value="osee2" />
		<Password key="#PASSWORD#" value="osee2" />
		<Host key="#HOST#" value="@AvailableDbServices.hostAddress" />
		<Port key="#PORT#" value="@AvailableDbServices.port" />
	</DatabaseInfo>

	<ConnectionDescription id="postgresql">
		<Driver>org.postgresql.Driver</Driver>
		<Url>#PREFIX#://#HOST#:#PORT#/#DBHOME##DBNAME#</Url>		
	</ConnectionDescription>
	<ConnectionDescription id="OracleClient">
		<Driver>oracle.jdbc.OracleDriver</Driver>
		<Url>#PREFIX#:@#HOST#:#PORT#:#DBHOME##DBNAME#</Url>
		<Property key="SetBigStringTryClob" value="true" />
	</ConnectionDescription>

	<AvailableDbServices>
		<Server id="postgresqlLocalhost" dbInfo="postgresqlInfo"
			hostAddress="localhost" port="5432"
			connectsWith="postgresql" resourceServer="http://localhost:8089/" />
		<Server id="oracleTest2" dbInfo="LbaOracleServerTest2"
			hostAddress="sun203.msc.az.boeing.com" port="1521"
			connectsWith="OracleClient" resourceServer="http://localhost:8089/" />
	</AvailableDbServices>

	<!-- This is here temporarily until we figure out how to tell the clients what to use -->
	<!-- Need to specify which authentication provider to use -->
	<AuthenticationProvider id="org.eclipse.osee.ats.config.demo.DemoAuthentication"/>
</OSEEConfig>